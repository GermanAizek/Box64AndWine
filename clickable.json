{
    "builder": "custom",
    "default": "build",
    "kill": "binarysupport",
    "dependencies_host": [
        "gcc-arm-linux-gnueabihf",
        "g++-arm-linux-gnueabihf",
        "debootstrap",
        "fakeroot",
        "fakechroot",
        "qemu-user-static",
        "libc6-dev-armhf-cross",
        "libx11-dev",
        "patchelf"
    ],
    "dependencies_target": [
        "libglvnd-dev",
        "libfuse2",
        "libwayland-dev",
        "libxmp-dev",
        "fuse",
        "wayland-protocols"
    ],
    "install_lib": [
        "/lib/${ARCH_TRIPLET}/libfuse.so*",
        "/lib/${ARCH_TRIPLET}/libulockmgr.so*"
    ],
    "install_bin": [
        "/usr/bin/fusermount"
    ],
    "build": "${ROOT}/build.sh -c",
    "make_jobs": 6,
    "qt_version": "5.12",
    "framework": "ubuntu-sdk-20.04"
}
